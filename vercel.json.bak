{
    "version": 2,
    "builds": [
      {
        "src": "portfolio.html",
        "use": "@vercel/static-build",
        "config": { "distDir": "public" }
      }
    ],
    "routes": [
      { "src": "/public/(.*)", "dest": "/public/$1" },
      { "src": "/(.*)", "dest": "/portfolio.html" }
    ]
  }